<!--https://imageserver.eveonline.com/Type/{0}_64.png-->
<view wx:for="{{itemData}}" wx:for-item="item">
  <view class="weui-cell">
    <view class="weui-cell__hd" style="position: relative;margin-right: 10px;">
      <image src="https://imageserver.eveonline.com/Type/{{item.TypeID}}_64.png" style="width: 50px; height: 50px; display: block" />      
    </view>
    <view class="weui-cell__bd">
      <view>{{item.Name}}</view>
      <view style="font-size: 13px;color: #888888;">出售价：{{item.MinSell}}</view>
      <view style="font-size: 13px;color: #888888;">收购价：{{item.MaxBuy}}</view>
    </view>    
  </view>
</view>